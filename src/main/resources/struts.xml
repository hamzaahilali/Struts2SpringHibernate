<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="default" namespace="/" extends="struts-default">
		<action name="LoginFormAction">
			<result>/Login.jsp</result>
		</action>

		<action name="LoginAction" class="com.web.action.LoginAction">
			<result type="redirectAction">
				<param name="actionName">HomeAction</param>
				<param name="namespace">/</param>
			</result>
			<result name="input">/Login.jsp</result>
		</action>


		<action name="RegisterFormAction">
			<result>/Register.jsp</result>
		</action>
		<action name="SuccessLoginAction" class="com.web.action.SuccessLoginAction">
			<result name="success">/SuccessLogin.jsp</result>
		</action>

		<action name="RegisterAction" class="com.web.action.RegisterAction">
			<result>/SuccessRegister.jsp</result>
			<result name="input">/Register.jsp</result>
		</action>

		<action name="HomeAction" class="com.web.action.HomeAction">
			<result> /HomePage.jsp</result>
		</action>

	</package>

</struts>